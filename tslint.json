{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "ban-types": false,
    "object-literal-sort-keys": false,
    "interface-name": false,
    "no-empty": [true, "allow-empty-catch", "allow-empty-functions"]
  }
}
